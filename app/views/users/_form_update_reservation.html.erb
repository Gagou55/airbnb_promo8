<%= form_for(@reservation, url: {action: "update"}) do |f| %>
<%= radio_button_tag(:validation, "true") %>
<%= label_tag(:true, "Welcome") %>
<%= f.submit "Accepte" %>
<%= radio_button_tag(:validation, "false") %>
<%= label_tag(:false, "Sorry, contact us in few days") %>
<%= f.submit "refuse" %>
<% end %>
