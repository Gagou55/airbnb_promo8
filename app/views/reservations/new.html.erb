<h1>Reservations</h1>
<div class="container">
  <div class="row centered">
    <div class="col-xs-12 col-md-6 col-md-offset-3 mb-100">
    <%= simple_form_for(@reservation, url: flat_reservations_path, method: :post) do |f| %>
      <%= f.input :begin_date, as: :string, input_html: { :class => "datepicker" } %>
      <%= f.input :end_date, as: :string, input_html: { :class => "datepicker" } %>

      <%= f.button :submit, class: "btn btn-primary" %>
    <% end %>

  <script type="text/javascript">
    $(function () {
    $('#datetimepicker1').datetimepicker();
    });
  </script>
   </div>
  </div>
</div>



